<?xml version="1.0" encoding="UTF-8"?>

<model>
<!--aquÃ­ se listan los objetos que aparecen en todo el escenario-->
  <alphabet>
    <v value="a"/>
    <v value="b"/>
    <v value="f"/>
    <v value="d"/>
    <v value="e"/>
  </alphabet>
<!--en este apartado se describen las reglas de cada membrana-->  
  <membranes>
    <membrane ID="1">
      <rBO pb="1.0" pr="1.0">
        <lh>
          <BO v="d" m="1">
        </lh>
        <rh type="bo" move="OUT">
          <BO v="d" m="1">
        </rh>
      </rBO>
    </membrane>

    <membrane ID="2">
      <rBO pb="1.0" pr="1.5">
        <lh>
          <BO v="b" m="1">
        </lh>
        <rh type="bo" move="HERE">
          <BO v="d" m="1">
        </rh>
      </rBO>

      <rBO pb="1.0" pr="1.5">
        <lh>
          <BO v="d" m="1">
        </lh>
        <rh type="bo" move="HERE">
          <BO v="d" m="1">
          <BO v="e" m="1">
        </rh>
      </rBO>

      <rBO pb="1.0" pr="1.5">
        <lh>
          <BO v="f" m="2">
        </lh>
        <rh type="bo" move="HERE">
          <BO v="f" m="1">
        </rh>
      </rBO>

      <rBO pb="1.0" pr="1.0">
        <lh>
          <BO v="f" m="1">
        </lh>
        <rh type="bo" move="DISS2">
        </rh>
      </rBO>
    </membrane>

    <membrane ID="3">
      <rBO pb="1.0" pr="1.0">
        <lh>
          <BO v="a" m="1">
        </lh>
        <rh type="bo" move="HERE">
          <BO v="a" m="1">
          <BO v="b" m="1">
        </rh>
      </rBO>

      <rBO pb="1.0" pr="1.0">
        <lh>
          <BO v="a" m="1">
        </lh>
        <rh type="bo" move="DISS2">
          <BO v="b" m="1">
        </rh>
      </rBO>

      <rBO pb="1.0" pr="1.0">
        <lh>
          <BO v="f" m="1">
        </lh>
        <rh type="bo" move="HERE">
          <BO v="f" m="2">
        </rh>
      </rBO>
    </membrane>

  </membranes>
</model>         
